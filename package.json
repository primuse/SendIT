{
  "name": "sendit",
  "version": "1.0.0",
  "description": "API for SendIT application",
  "main": "./src/index.js",
  "scripts": {
    "start": "node ./build/index.js",
    "dev": "nodemon ./src/index.js --exec babel-node",
    "build": "babel src --out-dir build",
    "test": "nyc --reporter=text mocha --require @babel/register ./src/tests/test.js --timeout 15000 --recursive --exit || true",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "postinstall": "npm run build && node db createTables && node db createAdmin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primuse/SendIT.git"
  },
  "keywords": [
    "api",
    "parcel",
    "sendit",
    "courier"
  ],
  "author": "Tiku Okoye",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/primuse/SendIT/issues"
  },
  "homepage": "https://github.com/primuse/SendIT#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@sendgrid/mail": "^6.3.1",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.0",
    "har-validator": "^5.1.3",
    "joi": "^14.0.6",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "make-runnable": "^1.3.6",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "pg": "^7.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "coveralls": "^3.0.2",
    "jsdoc": "^3.5.5",
    "nodemon": "^1.18.5",
    "nyc": "^13.1.0"
  },
  "engines": {
    "node": "10.11.0"
  }
}
